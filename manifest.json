{
  "name": "Quick YGG",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "Add a download button on the search page.",
  "content_scripts": [
    {
      "matches": ["https://*.yggtorrent.ws/*"],
      "js": ["quickygg.js"],
      "css": ["fix.css"],
      "run_at": "document_idle"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Quick YGG"
  }
}
